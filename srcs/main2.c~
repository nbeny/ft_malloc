#include "malloc.h"

int		main(int ac, char **av)
{
	int		i;
	int		j;
	char	**s;

	i = 0;
	s = NULL;
	if (ac > 1)
	{
		s = ft_malloc(sizeof(char *) * 500);
		while (av[i] != NULL)
		{
			s[i] = ft_malloc(sizeof(char) * 10);
			j = 0;
			while (j < 10)
			{
				s[i][j] = '*';
				j++;
			}
			s[i][j] = '\0';
			ft_printf(0, "%s\n", s[i]);
			i++;
		}
		if (g_pg == NULL)
			ft_printf(2, "!g_pg!\n");
		if (g_pg != NULL)
			ft_printf(2, "%p - %p\n", g_pg, g_pg->nxt);
		show_alloc_mem();
	}
	
	return (0);
}
