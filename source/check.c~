#include "malloc.h"

int		check_page_tiny(size_t size)
{
	t_page	*s;

	s = g_page;
	ft_printf(2, "%zu\n", size);
	while (s != NULL)
	{
//need add mac condition
		if (id == 0)
			return (0);
		s = s->next;
	}
	return (1);
}

int		check_page_tiny(size_t size)
{
	t_page	*s;

	s = g_page;
	ft_printf(2, "%zu\n", size);
	while (s != NULL)
	{
//need add mac condition
		if (id == 1)
			return (0);
		s = s->next;
	}
	return (1);
}
